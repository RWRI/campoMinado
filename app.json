{
  "expo": {
    "name": "minadex",
    "slug": "campo-minado",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logex.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashex.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logex.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rwri.campominado"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logex.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "beffaa47-8d64-41f6-b858-b29996dba06e"
      }
    }
  }
}
